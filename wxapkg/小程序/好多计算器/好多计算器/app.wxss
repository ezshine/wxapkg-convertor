}

.cu-btn[class*=lines]:after {
    border: 6rpx solid;
}

.cu-btn[class*=bg-]:after {
    display: none;
}

.cu-btn.sm {
    padding: 0 20rpx;
    font-size: 20rpx;
    height: 48rpx;
}

.cu-btn.lg {
