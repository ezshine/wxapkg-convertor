    height: 80rpx;
}

.cu-btn.shadow-blur:before {
    top: 4rpx;
    left: 4rpx;
    filter: blur(6rpx);
    opacity: .6;
}

.cu-btn.button-hover {
    transform: translate(1rpx,1rpx);
}

.block {
