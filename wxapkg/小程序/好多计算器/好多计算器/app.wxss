.scrollPage {
    height: 100vh;
}

.nav-list {
    display: flex;
    flex-wrap: wrap;
    padding: 0 40rpx;
    justify-content: space-between;
}

.nav-li {
    padding: 30rpx;
    border-radius: 12rpx;
    width: 45%;
    margin: 0 2.5% 40rpx;
    background-image: url(https://image.weilanwl.com/color2.0/cardBg.png);
    background-size: cover;
    background-position: 50%;
    position: relative;
    z-index: 1;
}

.nav-li:after {
    content: "";
    position: absolute;
    z-index: -1;
    background-color: inherit;
    width: 100%;
    height: 100%;
    left: 0;
    bottom: -10%;
    border-radius: 10rpx;
    opacity: .2;
    transform: scale(.9,.9);
}

.nav-li.cur {
    color: #fff;
    background: #5eb95e;
    box-shadow: 4rpx 4rpx 6rpx rgba(94,185,94,.4);
}

.nav-title {
    font-size: 32rpx;
    font-weight: 300;
}

.nav-title:first-letter {
    font-size: 40rpx;
    margin-right: 4rpx;
}

.nav-name {
    font-size: 28rpx;
    text-transform: Capitalize;
    margin-top: 20rpx;
    position: relative;
}

.nav-name:before {
    width: 40rpx;
    height: 6rpx;
    right: 0;
    opacity: .5;
}

.nav-name:after,.nav-name:before {
    content: "";
    position: absolute;
    display: block;
    background: #fff;
    bottom: 0;
}

.nav-name:after {
    width: 100rpx;
    height: 1px;
    right: 40rpx;
    opacity: .3;
}

.nav-name:first-letter {
    font-weight: 700;
    font-size: 36rpx;
    margin-right: 1px;
}

.nav-li text {
    position: absolute;
    right: 30rpx;
    top: 30rpx;
    font-size: 52rpx;
    width: 60rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
}

.text-light {
    font-weight: 300;
}